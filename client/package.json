{
  "name": "experts-client",
  "version": "0.1.0",
  "description": "Experts portal client",
  "main": "index.js",
  "scripts": {
    "start:old": "node server",
    "start": "babel-node ./tools/server",
    "open:dist": "babel-node tools/distServer.js",
    "clean": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "build:prod": "npm run prebuild && babel-node tools/build.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "cover": "jest --coverage",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamalight/building-products-with-js.git"
  },
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "tutorial",
    "front-end"
  ],
  "author": "Tim Ermilov <yamalight@gmail.com> (http://codezen.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yamalight/building-products-with-js/issues"
  },
  "homepage": "https://github.com/yamalight/building-products-with-js#readme",
  "jest": {
    "setupFiles": [
      "./tools/test/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "chalk": "^1.1.3",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.11.3",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "postcss-loader": "^2.0.5",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "connect-history-api-fallback": "^1.3.0",
    "create-react-class": "^15.5.1",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "prop-types": "^15.5.4",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.2.1",
    "webpack-bundle-analyzer": "^2.6.0"
  }
}
